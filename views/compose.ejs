<%- include('partials/header'); -%>

  <h1>Compose</h1>
  <form action="/compose" method="post">
    <label for="publish">Title</label>
    <div class="form-group">
      <input type="text" name="publish" id="publish" autocomplete="off" class="form-control">
    </div>
    <label for="textarea">Post</label>
    <div class="form-group">
      <textarea class="form-control" name="textarea" id="textarea" cols="30" rows="10" autocomplete="off"></textarea>

    </div>
    <button class="btn btn-success" type="submit" name="button">Publish</button>
  </form>

  <%- include('partials/footer'); -%>